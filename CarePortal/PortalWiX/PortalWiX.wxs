        <iis:WebAppPool Id="IrisAppPool" Name="IrisAppPool" ManagedPipelineMode="integrated" ManagedRuntimeVersion="v4.0" />
        <iis:WebVirtualDir Id="VirtualDir" Alias="[TARGETVDIR]" Directory="INSTALLLOCATION" WebSite="SelectedWebSite">
          <iis:WebApplication Id="IrisWebServs" Name="[TARGETVDIR]" WebAppPool="IrisAppPool" />
