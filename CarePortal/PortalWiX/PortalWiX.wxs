    <!-- Need to change INSTALLLOCATION to inetpub as if INSTALLLOCATION is removed we get error o2343-->
    <Property Id="INSTALLLOCATION" Secure="yes">
      <DirectorySearch Id="DirectorySearch1" Path="C:\inetpub\wwwroot\CarePortal" />
